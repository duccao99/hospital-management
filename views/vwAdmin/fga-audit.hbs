<div class="card border-light shadow-sm mb-4">
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-centered table-nowrap mb-0 rounded table-hover">


                <thead class="thead-light">
                    <tr>
                        <th class="border-0">DBUSERNAME</th>
                        <th class="border-0">ACTION_NAME</th>
                        <th class="border-0">SQL_TEXT</th>
                         <th class="border-0">OBJECT_SCHEMA</th>
                          <th class="border-0">OBJECT_NAME</th>
                           <th class="border-0">EVENT_TIMESTAMP</th>

                        {{!-- <th class="border-0">DELEGATE_OPTION</th> --}}
                    
                        {{!-- <th class="border-0">COMMON</th>
                        <th class="border-0">INHERITED</th> --}}

                    </tr>
                </thead>
                <tbody>
                    <!-- Item -->

                    {{#each fgaAudit }}
                    <tr>
                        <td class="border-0"><a href="#" class="text-primary font-weight-bold">
                                {{this.DBUSERNAME}}
                            </a>
                        </td>

                        <td class="border-0">
                            {{this.ACTION_NAME}}
                        </td>
                        <td class="border-0">
                            {{ this.SQL_TEXT}}
                        </td>

                        <td class="border-0">
                            {{this.OBJECT_SCHEMA}}
                        </td>
                             <td class="border-0">
                            {{this.OBJECT_NAME}}
                        </td>
                             <td class="border-0">
                            {{this.EVENT_TIMESTAMP}}
                        </td>





                    </tr>
                    {{/each}}



                </tbody>
            </table>
        </div>
    </div>
</div>